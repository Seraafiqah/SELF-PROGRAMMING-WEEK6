package mySwitch;

import java.util.Scanner;

public class myGradesSwitch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner (System.in);

		System.out.println("Enter grades A,B,C,D,E and F");
		String str = sc.next();
		char grade = str.charAt(0);
		
		switch(grade) {
		case 'A':
			System.out.println("Grade A mark >= 90");
			break;
			
		case 'B':
			System.out.println("Grade B mark >= 80 and less than 90");
			break;
			
		case 'C':
			System.out.println("Grade C mark >= 70 and less than 80");
			break;
		
		case 'D':
			System.out.println("Grade D mark >= 60 and less than 70");
			break;
			
		case 'E':
			System.out.println("Grade E mark >= 50 and less than 60");
			break;
			
		case 'F':
			System.out.println("Grade F mark <= 40");
			break;
			
		default:
			System.out.println("Invalid grade");
			
		}

	}

}
